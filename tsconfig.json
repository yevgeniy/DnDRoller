{
    "include": [
        "./src/*"
    ],
    "compilerOptions": {
        "lib": [
            "dom",
            "es2015"
        ],
        "jsx": "react"
    },
    "downlevelIteration": true,
    "noImplicitReturns": false,
    "noImplicitThis": false,
    "noImplicitAny": false,
    "noImplicitUseStrict": false,
    "string": false,
    "skipLibCheck": false,
}